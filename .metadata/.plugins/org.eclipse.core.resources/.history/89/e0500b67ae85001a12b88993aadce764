package top.biz;

import java.util.ArrayList;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;

import top.frame.Biz;
import top.frame.Dao;
import top.vo.OrderDetailVO;;

@Service("orderdetailbiz")
public class OrderDetailBiz implements Biz<String, OrderVO> {

	@Resource(name = "orderdao")
	Dao<String, OrderVO> dao;

	@Override
	public OrderVO get(String orderID) {
		return dao.select(orderID);
	}

	@Override
	public ArrayList<OrderVO> get() {
		return dao.selectall();
	}

	@Override
	public void register(OrderVO m) throws Exception {
		// TODO Auto-generated method stub
	}
}