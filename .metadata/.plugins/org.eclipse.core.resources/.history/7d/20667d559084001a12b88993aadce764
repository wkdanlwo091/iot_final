package algorithm;

public class Solution {
	
	  public String solution(int n) {
	      String answer = "";
	       if(n % 3 == 1) {
	    	   	answer = "1";
	       }
	      
	      return answer;
	  }

	public static void main(String[] args) {
		
		Solution a = new Solution();
		a.solution(3);
		
	}

}
